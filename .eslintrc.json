{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins":["prettier"],
    "rules": {
        "prettier/prettier":[
        "error",
        {
            "trailingComma": "es5",
            "arrowParens": "avoid",
            "endOfLine": "lf",
            "semi": true,
            "printWidth": 80,
            "tabWidth": 2,
            "singleQuote": true,
            "useTabs": false
    }
]
}
}
